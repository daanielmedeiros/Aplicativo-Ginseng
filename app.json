{
  "expo": {
    "name": "Grupo Ginseng",
    "slug": "ginseng-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2c65a577-710d-41af-8d59-78264aa3bda9"
      }
    },
    "android": {
      "package": "com.daanielmedeiros.ginsengapp"
    }
  }
}
